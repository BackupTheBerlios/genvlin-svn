<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/2">
            <code-name-base>de.genvlin.plugins.jsci</code-name-base>
            <suite-component/>
            <module-dependencies>
                <dependency>
                    <code-name-base>de.genvlin.core</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>1.0</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.modules</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>6.5.1</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.util</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>6.8.1</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>JSci</package>
                <package>JSci.maths</package>
                <package>JSci.maths.algebras</package>
                <package>JSci.maths.analysis</package>
                <package>JSci.maths.categories</package>
                <package>JSci.maths.chaos</package>
                <package>JSci.maths.fields</package>
                <package>JSci.maths.groups</package>
                <package>JSci.maths.matrices</package>
                <package>JSci.maths.polynomials</package>
                <package>JSci.maths.statistics</package>
                <package>JSci.maths.vectors</package>
                <package>JSci.awt</package>
                <package>JSci.swing</package>
                <package>JSci.chemistry.periodictable</package>
                <package>JSci.physics</package>
            </public-packages>
            <class-path-extension>
                <runtime-relative-path>ext/JSci.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/JSci.jar</binary-origin>
            </class-path-extension>
        </data>
    </configuration>
</project>
